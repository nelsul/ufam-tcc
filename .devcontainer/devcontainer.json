{
	"name": "C# (.NET) & PostgreSQL",
	"dockerComposeFile": "docker-compose.yml",
	"service": "main",
	"workspaceFolder": "/workspaces/ufam-tcc",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-dotnettools.csharp",
				"ms-dotnettools.csdevkit",
				"ms-dotnettools.vscode-dotnet-runtime",
				"csharpier.csharpier-vscode",
				"Vue.volar",
				"esbenp.prettier-vscode",
				"dbaeumer.vscode-eslint"
			]
		}
	},
	"postCreateCommand": "dotnet tool install -g csharpier",
	"forwardPorts": [
		5432,
		5000,
		5001,
		5173
	],
	"remoteUser": "vscode"
}